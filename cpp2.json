	"Sieve of Eratosthenes": {
		"prefix": "siv",
		"body": [
			"int N = 100000;",
			"vector<bool> Prime(N+1, true);",
			"void siv(int n)",
			"{",
			"    Prime[0] = false, Prime[1] = false;",
			"    for (int p = 2; p*p <= n; p++)",
			"    {",
			"        if (Prime[p] == true)",
			"        {",
			"            for (int i = p * p; i <= n; i+=p)",
			"            {",
			"                Prime[i] = false;",
			"			}",
			"		}",
			"	}",
			"}",
		],
		"description": "Sieve of Eratosthenes"
	},
	"Fectorial of big Integers": {
		"prefix": "Big_Factorial",
		"body": [
			"void multiply(vector<int> &res, int x)",
			"{",
			"    int carry = 0;",
			"    for (int i = 0; i < res.size(); i++)",
			"    {",
			"        int prod = res[i] * x + carry;",
			"        res[i] = prod % 10;",
			"        carry = prod / 10;",
			"    }",
			"    while (carry)",
			"    {",
			"        res.push_back(carry % 10);",
			"        carry /= 10;",
			"    }",
			"}",
			"vector<int> factorial(int n)",
			"{",
			"    vector<int> res;",
			"    res.push_back(1);",
			"    for (int x = 2; x <= n; x++)",
			"    {",
			"        multiply(res, x);",
			"    }",
			"    reverse(res.begin(), res.end());",
			"    return res;",
			"}"
		],
		"description": "Fectprial of big Integers"
	},
